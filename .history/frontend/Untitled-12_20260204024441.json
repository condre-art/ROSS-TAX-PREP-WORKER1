{
  "name": "ross-tax-prep-worker",
  "main": "src/index.ts",
  "compatibility_date": "2024-01-01",
  "observability": {
    "logs": {
      "enabled": false,
      "invocation_logs": true
    }
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "ross_tax_db",
      "database_id": "your-d1-database-id"
    }
  ],
  "r2_buckets": [
    {
      "binding": "DOCUMENTS",
      "bucket_name": "ross-tax-documents"
    }
  ],
  "kv_namespaces": [
    {
      "binding": "SESSIONS",
      "id": "your-kv-namespace-id"
    }
  ],
  "vars": {
    "ENVIRONMENT": "development",
    "APP_URL": "https://rosstaxprepandbookkeeping.com",
    "MEF_ENVIRONMENT": "ATS"
  },
  "build": {
    "command": "npm run build"
  },
  "routes": [
    {
      "pattern": "rosstaxprepandbookkeeping.com/*",
      "zone_name": "rosstaxprepandbookkeeping.com"
    }
  ]
}